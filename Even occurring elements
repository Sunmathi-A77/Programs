class Solution:
    def findEvenOccurrences(self, arr):
        
        freq = {}
        result = []

        for num in arr:
            freq[num] = freq.get(num, 0) + 1
    
        added = set()
        for num in arr:
            if freq[num] % 2 == 0 and num not in added:
                result.append(num)
                added.add(num)
    
        if not result:
            return [-1]
    
        return result
