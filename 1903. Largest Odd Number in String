char* largestOddNumber(char* num) {
    
    int n = strlen(num);

    for (int i = n - 1; i >= 0; i--) 
    {
        if ((num[i] - '0') % 2 == 1) 
        {
            num[i + 1] = '\0';
            return num;
        }
    }

    num[0] = '\0';
    return num;
}
