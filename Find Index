// Java

class Solution {
    // Function to find starting and end index
    static int[] findIndex(int arr[], int key) {
        int start=-1,end=-1;
        for(int i=0;i<arr.length;i++)
        {
            if(key==arr[i])
            {
                if(start==-1)
                {
                    start=i;
                }
                end=i;
            }
        }
        return new int[] {start,end};
    }
}
