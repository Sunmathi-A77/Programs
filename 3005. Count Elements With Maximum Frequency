class Solution(object):
    def maxFrequencyElements(self, nums):

        freq = Counter(nums)
        maxFreq = max(freq.values())
        total = sum(c for c in freq.values() if c == maxFreq)
        return total




int maxFrequencyElements(int* nums, int numsSize) {

    int freq[101] = {0};

    for (int i = 0; i < numsSize; i++) 
    {
        freq[nums[i]]++;
    }

    int maxFreq = 0;
    for (int i = 1; i <= 100; i++) 
    {
        if (freq[i] > maxFreq) 
        {
            maxFreq = freq[i];
        }
    }

    int total = 0;
    for (int i = 1; i <= 100; i++) 
    {
        if (freq[i] == maxFreq) 
        {
            total += freq[i];
        }
    }

    return total;
    
}
