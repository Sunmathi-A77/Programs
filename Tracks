class Solution {

    public boolean ValidTrack(int[] arr) {
        
        int n= arr.length;
        
        for(int i=0; i<n/2; i++)
        {
            int j=n-i-1;
            if(arr[i]!=arr[j])
            {
                return false;
            }
        }
        
        return true;
    }
}
