int maxProfit(int* prices, int pricesSize) {

    int minprice=INT_MAX;
    int max_profit=0;

    for(int i=0;i<pricesSize;i++)
    {
        if(prices[i]<minprice)
        {
            minprice=prices[i];
        }
        else
        {
            int profit=prices[i]-minprice;
            if(profit>max_profit)
            {
                max_profit=profit;
            }
        }
    }
    return max_profit;
}
