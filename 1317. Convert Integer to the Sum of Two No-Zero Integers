int hasZero(int n)
{
    while(n>0)
    {
        if(n % 10 == 0)
        {
            return true;
        }
        n /= 10;
    }

    return false;
}


int* getNoZeroIntegers(int n, int* returnSize) {

    int static result[2];
    *returnSize = 2;

    for(int i=1;i<n;i++)
    {
        int j=n-i;

        if(!hasZero(i) && !hasZero(j))
        {
            result[0]=i;
            result[1]=j;
            break;
        }
    }

    return result;
}
