class Solution:

    def countryAtWar(self, arr1, arr2):
        
        n = len(arr1)
        
        count_A = 0
        count_B = 0
        
        for i in range(n):
            if arr1[i] > arr2[i]:
                count_A += 1
            elif arr1[i] < arr2[i]:
                count_B += 1
                
        if count_A > count_B:
            return "A"
        elif count_A < count_B:
            return "B"
        else:
            return "DRAW"
