int gcd(int a,int b)
{
    return (b==0) ? a : gcd(b, a % b); 
}

char* gcdOfStrings(char* str1, char* str2) {

    static char result[1001];

    int len1=strlen(str1);
    int len2=strlen(str2);

    char concat1[2001];
    char concat2[2001];

    strcpy(concat1,str1);
    strcat(concat1,str2);    
    strcpy(concat2,str2);    
    strcat(concat2,str1);    

    if(strcmp(concat1,concat2)!=0)
    {
        return "";
    }

    int g = gcd(len1,len2);

    strncpy(result,str1,g);
    result[g]='\0';

    return result;

}
