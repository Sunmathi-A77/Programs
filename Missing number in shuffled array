        return arr1[-1]
        
        
        
class Solution:
    def findMissing(self, arr1,arr2):
        res = 0
        for num in arr1:
            res ^= num
        for num in arr2:
            res ^= num
        return res
        
"""
        

class Solution:
    def findMissing(self, arr1, arr2):
        
        return sum(arr1) - sum(arr2)
