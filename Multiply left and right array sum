// User function Template for Java

class Solution {
    public int multiply(int[] arr) {
        
        int mid=arr.length/2;
        int sumleft=0,sumright=0;
        
        for(int i=0;i<arr.length;i++)
        {
            if(i<mid)
            {
                sumleft+=arr[i];
            }
            else 
            {
                sumright+=arr[i];
            }
        }
        
        return sumleft*sumright;
    }
}
