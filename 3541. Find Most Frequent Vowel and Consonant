int maxFreqSum(char* s) {

    int freq[26] = {0};
    int n = strlen(s);

    for (int i = 0; i < n; i++) 
    {
        freq[s[i] - 'a']++;
    }

    int maxVowel = 0, maxConsonant = 0;

    for (int i = 0; i < 26; i++) 
    {
        char ch = 'a' + i;
        if (freq[i] > 0) 
        {
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') 
            {
                if (freq[i] > maxVowel) 
                {
                    maxVowel = freq[i];
                }
            } 
            else 
            {
                if (freq[i] > maxConsonant) 
                {
                    maxConsonant = freq[i];
                }
            }
        }
    }

    return maxVowel + maxConsonant;
    
}
