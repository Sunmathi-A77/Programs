class Solution {
    public int firstElementKTime(int[] arr, int k) {
        
        int maxval=100000;
        
        int[] frq=new int[maxval];
        
        for(int i=0;i<arr.length;i++)
        {
            frq[arr[i]]++;
            
            if(frq[arr[i]]==k)
            {
                return arr[i];
            }
        }
        
        return -1;
    }
}
