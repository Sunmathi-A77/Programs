int sumOddLengthSubarrays(int* arr, int arrSize) {

    int n=arrSize;
    int total=0;

    for(int i=0;i<n;i++)
    {
        for(int j=i;j<n;j++)
        {
            int len=j-i+1;

            if(len%2==1)
            {
                for(int k=i;k<=j;k++)
                {
                    total+=arr[k];
                }
            }
        }
    }
    return total;
}



